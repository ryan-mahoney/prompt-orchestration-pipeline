{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "reaction",
    "insightByArea"
  ],
  "properties": {
    "reaction": {
      "type": "string",
      "minLength": 1
    },
    "insightByArea": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "area",
          "alternatePerspective",
          "implications"
        ],
        "properties": {
          "area": {
            "type": "string",
            "minLength": 1
          },
          "alternatePerspective": {
            "type": "string",
            "minLength": 1
          },
          "implications": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  }
}