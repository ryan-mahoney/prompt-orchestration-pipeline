{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "researchSummary",
    "keyFindings",
    "additionalInsights",
    "criticalPerspectives",
    "researchCompleteness"
  ],
  "properties": {
    "researchSummary": {
      "type": "string",
      "minLength": 1
    },
    "keyFindings": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": true,
        "required": [
          "area",
          "findings"
        ],
        "properties": {
          "area": {
            "type": "string",
            "minLength": 1
          },
          "findings": {
            "type": "string",
            "minLength": 1
          },
          "sources": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          }
        }
      }
    },
    "additionalInsights": {
      "type": "string"
    },
    "criticalPerspectives": {
      "type": "string",
      "minLength": 1
    },
    "researchCompleteness": {
      "type": "string",
      "minLength": 1
    }
  }
}